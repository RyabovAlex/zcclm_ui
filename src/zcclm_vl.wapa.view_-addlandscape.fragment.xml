<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">                                                                                                                                                                  
	<Dialog title="{i18n>addLandscapeTitle}" afterClose="onDialogClose">                                                                                                                                                                                          
		<VBox class="sapUiSmallMargin">                                                                                                                                                                                                                              
			<f:SimpleForm id="SimpleForm" minWidth="256" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" labelSpanL="3"                                                                                                                              
				labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1" class="editableForm">                                                                                                                                                               
				<f:content>                                                                                                                                                                                                                                                
					<Label text="{i18n>landscapeName}"/>                                                                                                                                                                                                                      
					<Input id="newName" value="{LSName}" valueLiveUpdate="true" liveChange="onType"/>                                                                                                                                                                         
					<Label text="{i18n>landscapeDesc}"/>                                                                                                                                                                                                                      
					<Input id="newDescription" value="{LSDesc}"></Input>                                                                                                                                                                                                      
				</f:content>                                                                                                                                                                                                                                               
			</f:SimpleForm>                                                                                                                                                                                                                                             
		</VBox>                                                                                                                                                                                                                                                      
		<beginButton>                                                                                                                                                                                                                                                
			<Button text="{i18n>submit}" id="Submit" enabled="false" press="onCommitLandscape"/>                                                                                                                                                                        
		</beginButton>                                                                                                                                                                                                                                               
		<endButton>                                                                                                                                                                                                                                                  
			<Button text="{i18n>cancel}" press="onCancel"/>                                                                                                                                                                                                             
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     