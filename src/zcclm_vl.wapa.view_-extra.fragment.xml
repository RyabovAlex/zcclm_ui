<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">                                                                                                                                                                                               
	<Dialog title="Warning!" icon="sap-icon://warning2" state="Warning">                                                                                                                                                                                          
		<content>                                                                                                                                                                                                                                                    
			<Table headerText="Added systems are present in another landscapes"                                                                                                                                                                                         
				items="{path: '/data'}">                                                                                                                                                                                                                                   
				<columns>                                                                                                                                                                                                                                                  
					<Column mergeDuplicates="true">                                                                                                                                                                                                                           
						<Text text="System"/>                                                                                                                                                                                                                                    
					</Column>                                                                                                                                                                                                                                                 
					<Column>                                                                                                                                                                                                                                                  
						<Text text="Landscape"/>                                                                                                                                                                                                                                 
					</Column>                                                                                                                                                                                                                                                 
				</columns>                                                                                                                                                                                                                                                 
				<ColumnListItem>                                                                                                                                                                                                                                           
					<Text text="{sys}" />                                                                                                                                                                                                                                     
					<Text text="{ln}" />                                                                                                                                                                                                                                      
				</ColumnListItem>                                                                                                                                                                                                                                          
			</Table>                                                                                                                                                                                                                                                    
		</content>                                                                                                                                                                                                                                                   
		<buttons>                                                                                                                                                                                                                                                    
			<Button id="idCloseExtra" text="OK" press="onCloseExtra" />                                                                                                                                                                                                 
		</buttons>                                                                                                                                                                                                                                                   
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     