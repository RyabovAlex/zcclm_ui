<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">                                                                                                                                                                                               
	<Dialog title="{i18n>deleteLandscapeTitle}" afterClose="onDelLSDialogClose">                                                                                                                                                                                  
		<Label text="{i18n>deleteConfirm}" class="sapUiSmallMarginBegin"/>                                                                                                                                                                                           
		<beginButton>                                                                                                                                                                                                                                                
		    <Button text="{i18n>yes}" type="Reject" press="onRemoveLandscape"/>                                                                                                                                                                                      
		</beginButton>                                                                                                                                                                                                                                               
		<endButton>                                                                                                                                                                                                                                                  
		    <Button text="{i18n>no}" press="onCancel"/>                                                                                                                                                                                                              
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     