<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:core="sap.ui.core">                                                                                                                                                                                                                                     
	<Dialog title="{i18n>addLandscapeTitle}">                                                                                                                                                                                                                     
		<VBox class="sapUiSmallMargin">                                                                                                                                                                                                                              
				<Input                                                                                                                                                                                                                                                     
				id="emailInput"                                                                                                                                                                                                                                            
				class="sapUiSmallMarginBottom"                                                                                                                                                                                                                             
				type="Email"                                                                                                                                                                                                                                               
				placeholder="Enter E-Mail ..."                                                                                                                                                                                                                             
				/>                                                                                                                                                                                                                                                         
				<Button text="Add" press="onAddEmail"/>                                                                                                                                                                                                                    
			<Table id="table" width="auto" class="sapUiResponsiveMargin" items="{/EMAILSet}">                                                                                                                                                                           
				<headerToolbar>                                                                                                                                                                                                                                            
					<Toolbar>                                                                                                                                                                                                                                                 
						<Title text="{i18n>detailHeader}"/>                                                                                                                                                                                                                      
					</Toolbar>                                                                                                                                                                                                                                                
				</headerToolbar>                                                                                                                                                                                                                                           
				<columns>                                                                                                                                                                                                                                                  
					<Column>                                                                                                                                                                                                                                                  
						<Text text="Email"/>                                                                                                                                                                                                                                     
					</Column>                                                                                                                                                                                                                                                 
				</columns>                                                                                                                                                                                                                                                 
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem>                                                                                                                                                                                                                                          
						<cells>                                                                                                                                                                                                                                                  
							<Text text="{mail}"/>                                                                                                                                                                                                                                   
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
			</Table>		                                                                                                                                                                                                                                                  
		</VBox>                                                                                                                                                                                                                                                      
		<beginButton>                                                                                                                                                                                                                                                
		    <Button text="OK" id="Ok" enabled="false"  press="onCommitLandscape"/>                                                                                                                                                                                   
		</beginButton>                                                                                                                                                                                                                                               
		<endButton>                                                                                                                                                                                                                                                  
		    <Button text="Cancel" press="onCancel"/>                                                                                                                                                                                                                 
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     