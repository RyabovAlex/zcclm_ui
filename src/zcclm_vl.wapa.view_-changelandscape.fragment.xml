<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">                                                                                                                                                                  
	<Dialog title="Change landscape">                                                                                                                                                                                                                             
		<VBox class="sapUiSmallMargin">                                                                                                                                                                                                                              
			<f:SimpleForm id="SimpleForm" minWidth="256" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" labelSpanL="3"                                                                                                                              
				labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1" class="editableForm">                                                                                                                                                               
				<f:content>                                                                                                                                                                                                                                                
					<Label text="New landscape name"/>                                                                                                                                                                                                                        
					<Input id="changeName" value="{LSName}" valueLiveUpdate="true" liveChange="onType"/>                                                                                                                                                                      
					<Label text="New description"/>                                                                                                                                                                                                                           
					<Input id="changeDescription" value="{LSDesc}"></Input>                                                                                                                                                                                                   
				</f:content>                                                                                                                                                                                                                                               
			</f:SimpleForm>                                                                                                                                                                                                                                             
		</VBox>                                                                                                                                                                                                                                                      
		<beginButton>                                                                                                                                                                                                                                                
			<Button text="Change" id="Submit" press="onEditLandscape"/>                                                                                                                                                                                                 
		</beginButton>                                                                                                                                                                                                                                               
		<endButton>                                                                                                                                                                                                                                                  
			<Button text="Cancel" press="onCancelChange"/>                                                                                                                                                                                                              
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     