<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:core="sap.ui.core">                                                                                                                                                                                                                                     
	<SelectDialog                                                                                                                                                                                                                                                 
	    id="deleteSystemDialog"                                                                                                                                                                                                                                   
		noDataText="{i18n>noSystemsFound}"                                                                                                                                                                                                                           
		title="{i18n>selectSystem}"                                                                                                                                                                                                                                  
		multiSelect="true"                                                                                                                                                                                                                                           
		search="handleSearch"                                                                                                                                                                                                                                        
		confirm="onSystemDelete"                                                                                                                                                                                                                                     
		cancel="onDeleteSystemDialogClose"                                                                                                                                                                                                                           
		>                                                                                                                                                                                                                                                            
		<StandardListItem                                                                                                                                                                                                                                            
			title="{Id}"                                                                                                                                                                                                                                                
			type="Active" />                                                                                                                                                                                                                                            
	</SelectDialog>                                                                                                                                                                                                                                               
</core:FragmentDefinition>                                                                                                                                                                                                                                     