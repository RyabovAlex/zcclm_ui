<mvc:View	xmlns:core="sap.ui.core"                                                                                                                                                                                                                             
			xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                 
			xmlns="sap.m"                                                                                                                                                                                                                                               
			xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                                   
			xmlns:semantic="sap.m.semantic"                                                                                                                                                                                                                             
			controllerName="sap.cust.zcclm_cm.controller.Landscapes"                                                                                                                                                                                                    
			>                                                                                                                                                                                                                                                           
	<semantic:FullscreenPage	id="page"                                                                                                                                                                                                                            
								title="{i18n>title}"                                                                                                                                                                                                                                   
								>                                                                                                                                                                                                                                                      
		<semantic:content>                                                                                                                                                                                                                                           
			<IconTabBar id="idIconTabBarNoIcons"                                                                                                                                                                                                                        
						expanded="{device>/isNoPhone}"                                                                                                                                                                                                                           
						class="sapUiResponsiveContentPadding"                                                                                                                                                                                                                    
						>                                                                                                                                                                                                                                                        
				<items>                                                                                                                                                                                                                                                    
					<IconTabFilter text="Solution Manager">                                                                                                                                                                                                                   
						<VBox>                                                                                                                                                                                                                                                   
							<Table id="idSolmanNoteTable" items="{path: '/SOLMAN_NOTESet'}" class="sapUiMediumMarginBottom" fixedLayout="false" noDataText="All critical CCLM notes are implemented on Solution Manager">                                                           
								<headerToolbar>                                                                                                                                                                                                                                        
									<Toolbar>                                                                                                                                                                                                                                             
										<content>                                                                                                                                                                                                                                            
											<Title text="Solution Manager notes" level="H2"/>                                                                                                                                                                                                   
										</content>                                                                                                                                                                                                                                           
									</Toolbar>                                                                                                                                                                                                                                            
								</headerToolbar>                                                                                                                                                                                                                                       
								<columns>                                                                                                                                                                                                                                              
									<Column>                                                                                                                                                                                                                                              
										<Text text="Missing Note"/>                                                                                                                                                                                                                          
									</Column>                                                                                                                                                                                                                                             
									<Column hAlign="Center" width="20%">                                                                                                                                                                                                                  
										<Text text="Link"/>                                                                                                                                                                                                                                  
									</Column>                                                                                                                                                                                                                                             
								</columns>                                                                                                                                                                                                                                             
								<items>                                                                                                                                                                                                                                                
									<ColumnListItem vAlign="Middle">                                                                                                                                                                                                                      
										<cells>                                                                                                                                                                                                                                              
											<Text text="{Resultmsg}"/>                                                                                                                                                                                                                          
											<Link text="Launchpad" href="{ResolutionLink}"/>                                                                                                                                                                                                    
										</cells>                                                                                                                                                                                                                                             
									</ColumnListItem>                                                                                                                                                                                                                                     
								</items>                                                                                                                                                                                                                                               
							</Table>                                                                                                                                                                                                                                                
							<Table id="idSolmanBWTable" items="{path: '/SOLMAN_BWSet'}" class="sapUiMediumMarginBottom" fixedLayout="false" noDataText="Cannot check BW infrastructure">                                                                                            
								<headerToolbar>                                                                                                                                                                                                                                        
									<Toolbar>                                                                                                                                                                                                                                             
										<content>                                                                                                                                                                                                                                            
											<Title text="BW infrastructure" level="H2"/>                                                                                                                                                                                                        
										</content>                                                                                                                                                                                                                                           
									</Toolbar>                                                                                                                                                                                                                                            
								</headerToolbar>                                                                                                                                                                                                                                       
								<columns>                                                                                                                                                                                                                                              
									<Column>                                                                                                                                                                                                                                              
										<Text text="BW Cube"/>                                                                                                                                                                                                                               
									</Column>                                                                                                                                                                                                                                             
									<Column hAlign="Center" width="20%">                                                                                                                                                                                                                  
										<Text text="Status"/>                                                                                                                                                                                                                                
									</Column>                                                                                                                                                                                                                                             
								</columns>                                                                                                                                                                                                                                             
								<items>                                                                                                                                                                                                                                                
									<ColumnListItem vAlign="Middle">                                                                                                                                                                                                                      
										<cells>                                                                                                                                                                                                                                              
											<Text text="{Scenario}"/>                                                                                                                                                                                                                           
											<core:Icon src="{ path: 'IsActive',              formatter: '.getIconTypeBW' }" color="{ path: 'IsActive',                formatter: '.getIconColorBW' }" size="2em"/>                                                                              
										</cells>                                                                                                                                                                                                                                             
									</ColumnListItem>                                                                                                                                                                                                                                     
								</items>                                                                                                                                                                                                                                               
							</Table>                                                                                                                                                                                                                                                
							<Table id="idSolmanJobsTable" items="{path: '/SOLMAN_JOBSet'}" class="sapUiMediumMarginBottom" fixedLayout="false" noDataText="Required jobs are not found on Solution Manager">                                                                        
								<headerToolbar>                                                                                                                                                                                                                                        
									<Toolbar>                                                                                                                                                                                                                                             
										<content>                                                                                                                                                                                                                                            
											<Title text="Solution Manager jobs" level="H2"/>                                                                                                                                                                                                    
										</content>                                                                                                                                                                                                                                           
									</Toolbar>                                                                                                                                                                                                                                            
								</headerToolbar>                                                                                                                                                                                                                                       
								<columns>                                                                                                                                                                                                                                              
									<Column>                                                                                                                                                                                                                                              
										<Text text="Name"/>                                                                                                                                                                                                                                  
									</Column>                                                                                                                                                                                                                                             
									<Column hAlign="Center">                                                                                                                                                                                                                              
										<Text text="Scheduled start"/>                                                                                                                                                                                                                       
									</Column>                                                                                                                                                                                                                                             
									<Column hAlign="Center">                                                                                                                                                                                                                              
										<Text text="Periodical value (in min.)"/>                                                                                                                                                                                                            
									</Column>                                                                                                                                                                                                                                             
									<Column hAlign="Center" width="20%">                                                                                                                                                                                                                  
										<Text text="Overall status"/>                                                                                                                                                                                                                        
									</Column>                                                                                                                                                                                                                                             
								</columns>                                                                                                                                                                                                                                             
								<items>                                                                                                                                                                                                                                                
									<ColumnListItem vAlign="Middle" type="Active" press="onSolmanJobPress">                                                                                                                                                                               
										<cells>                                                                                                                                                                                                                                              
											<Text text="{Jobname}"/>                                                                                                                                                                                                                            
											<Text text="{Start}"/>                                                                                                                                                                                                                              
											<Text text="{Period}"/>                                                                                                                                                                                                                             
											<core:Icon src="{ path: 'Status',              formatter: '.getIconTypeJob' }" color="{ path: 'Status',                formatter: '.getIconColorJob' }" size="2em"/>                                                                                
										</cells>                                                                                                                                                                                                                                             
									</ColumnListItem>                                                                                                                                                                                                                                     
								</items>                                                                                                                                                                                                                                               
							</Table>                                                                                                                                                                                                                                                
						</VBox>                                                                                                                                                                                                                                                  
					</IconTabFilter>                                                                                                                                                                                                                                          
					<IconTabFilter text="Managed Systems">                                                                                                                                                                                                                    
						<Table id="idLandscapesTable" mode="SingleSelectMaster" itemPress="onLandscapeSelection" items="{path: '/LANDSCAPE_STATUSSet'}" class="sapUiMediumMarginBottom" noDataText="Connect managed systems and configure landscapes">                           
							<headerToolbar>                                                                                                                                                                                                                                         
								<Toolbar>                                                                                                                                                                                                                                              
									<content>                                                                                                                                                                                                                                             
										<Title text="Connected landscapes" level="H2"/>                                                                                                                                                                                                      
									</content>                                                                                                                                                                                                                                            
								</Toolbar>                                                                                                                                                                                                                                             
							</headerToolbar>                                                                                                                                                                                                                                        
							<columns>                                                                                                                                                                                                                                               
								<Column>                                                                                                                                                                                                                                               
									<Text text="Landscape"/>                                                                                                                                                                                                                              
								</Column>                                                                                                                                                                                                                                              
								<Column hAlign="Center">                                                                                                                                                                                                                               
									<Text text="Analysis jobs"/>                                                                                                                                                                                                                          
								</Column>                                                                                                                                                                                                                                              
								<Column hAlign="Center">                                                                                                                                                                                                                               
									<Text text="Extractors"/>                                                                                                                                                                                                                             
								</Column>                                                                                                                                                                                                                                              
								<Column hAlign="Center">                                                                                                                                                                                                                               
									<Text text="Collectors"/>                                                                                                                                                                                                                             
								</Column>                                                                                                                                                                                                                                              
								<Column hAlign="Center">                                                                                                                                                                                                                               
									<Text text="Notes"/>                                                                                                                                                                                                                                  
								</Column>                                                                                                                                                                                                                                              
							</columns>                                                                                                                                                                                                                                              
							<items>                                                                                                                                                                                                                                                 
								<ColumnListItem vAlign="Middle" type="Navigation">                                                                                                                                                                                                     
									<cells>                                                                                                                                                                                                                                               
										<ObjectIdentifier title="{LandscapeId}" text="{LandscapeDescription}"/>                                                                                                                                                                              
										<core:Icon src="{ path: 'JobStatus',              formatter: '.getIconType' }" color="{ path: 'JobStatus',                formatter: '.getIconColor' }" size="2em"/>                                                                                 
										<core:Icon src="{ path: 'ExtractorStatus',              formatter: '.getIconType' }" color="{ path: 'ExtractorStatus',                formatter: '.getIconColor' }" size="2em"/>                                                                     
										<core:Icon src="{ path: 'CollectorStatus',              formatter: '.getIconType' }" color="{ path: 'CollectorStatus',                formatter: '.getIconColor' }" size="2em"/>                                                                     
										<core:Icon src="{ path: 'NoteStatus',              formatter: '.getIconType' }" color="{ path: 'NoteStatus',                formatter: '.getIconColor' }" size="2em"/>                                                                               
									</cells>                                                                                                                                                                                                                                              
								</ColumnListItem>                                                                                                                                                                                                                                      
							</items>                                                                                                                                                                                                                                                
						</Table>                                                                                                                                                                                                                                                 
					</IconTabFilter>                                                                                                                                                                                                                                          
				</items>                                                                                                                                                                                                                                                   
			</IconTabBar>                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
		</semantic:content>                                                                                                                                                                                                                                          
		<semantic:customFooterContent>                                                                                                                                                                                                                               
 			<Button text="Trigger update" icon="sap-icon://restart" class="sapUiSmallMargin" press="onUpdPress"/>                                                                                                                                                      
			<Button press="action" text="Best practices" icon="sap-icon://settings" class="sapUiSmallMargin" xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" action:wiring="\{'press':\{'navigation':\{'routeName':'BestPractice'\}\}\}+
"/>                                                                                                                                                                                                                                                            
		</semantic:customFooterContent>                                                                                                                                                                                                                              
	</semantic:FullscreenPage>                                                                                                                                                                                                                                    
</mvc:View>                                                                                                                                                                                                                                                    